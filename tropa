
function createSinglePixelImageWithDifferentValues() {
  // Create a list of 21 random values
  var randomValues = ee.List.sequence(1, 21).map(function(i) {
    return Random.number(0, 100); // Adjust range as needed
  });

  // Create a constant image for each value
  var imageList = randomValues.map(function(value) {
    return ee.Image.constant(value);
  });

  // Combine images into a single image with 21 bands
  var imageWithBands = ee.ImageCollection(imageList).toImage();

  return imageWithBands;
}

// Create the single-pixel image with different values
var singlePixelImage = createSinglePixelImageWithDifferentValues();

// Reduce the image to a single band using reducer sum
var summedImage = singlePixelImage.reduce(ee.Reducer.sum());

// Print the summed image
print(summedImage);